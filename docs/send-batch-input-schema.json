{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/sszlachetka/ServiceBusAdmin/tree/master/docs/send-batch-input-schema.json",
  "type": "object",
  "properties": {
    "metadata": {
      "type": "object",
      "properties": {
        "messageId": {
          "type": "string"
        },
        "applicationProperties": {
          "type": "object",
          "patternProperties": {
            ".+": {
              "type": ["string", "number", "integer", "null", "boolean"]
            }
          }
        }
      }
    },
    "body": {
      "type": ["object", "string"]
    }
  },
  "required": ["body"]
}